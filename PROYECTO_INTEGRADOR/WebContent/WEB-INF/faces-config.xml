<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_3.xsd"
    version="2.3">
	<navigation-rule>
		<display-name>Index.xhtml</display-name>
		<from-view-id>/Index.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>invitado</from-outcome>
			<to-view-id>/Invitado.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>Invitado.xhtml</display-name>
		<from-view-id>/Invitado.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>info</from-outcome>
			<to-view-id>/Info.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>Invitado.xhtml</display-name>
		<from-view-id>/Invitado.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>reportes</from-outcome>
			<to-view-id>/ReportesP.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>ReportesP.xhtml</display-name>
		<from-view-id>/ReportesP.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>invi</from-outcome>
			<to-view-id>/Invitado.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>Info.xhtml</display-name>
		<from-view-id>/Info.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>autor</from-outcome>
			<to-view-id>/Autores.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
</faces-config>